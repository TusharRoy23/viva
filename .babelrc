{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        ["module-resolver", {
            "root": ["./"],
            alias: {
                "@": "./",
                "@client": "./client",
                "@server": "./server",
                "@store": "./client/store",
                "@components":"./client/component",
                "@pages": "./client/pages",
                "@common": "./client/pages/common",
                "@config": "./server/config",
                "@routes" : "./server/routes",
                "@validators": "./server/validator",
                "@controllers" : "./server/controller",
                "@data": "./data"
            }
        }]
    ]
} 